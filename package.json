{
	"name": "k_on_line",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "nodemon --watch middleware.ts --exec next dev",
		"build": "next build",
		"start": "next start",
		"lint": "eslint --ext .ts,.tsx src",
		"lint:fix": "eslint --ext .ts,.tsx src --fix",
		"format": "prettier --check --ignore-path .gitignore .",
		"format:fix": "prettier --write --ignore-path .gitignore .",
		"seed": "dotenv -e .env.test -- node src/lib/prisma/seed.ts",
		"migrate:init": "dotenv -e .env.test -- npx prisma migrate dev --name postgres-init",
		"migrate:add": "dotenv -e .env.test -- npx prisma migrate dev",
		"migrate:reset": "dotenv -e .env.test -- npx prisma migrate reset",
		"generate": "dotenv -e .env.test -- npx prisma generate",
		"db:push": "dotenv -e .env.test -- npx prisma db push"
	},
	"dependencies": {
		"@auth/prisma-adapter": "^2.7.4",
		"@hookform/resolvers": "^3.6.0",
		"@line/liff": "^2.23.2",
		"@line/liff-inspector": "^1.0.2",
		"@netlify/functions": "^3.0.0",
		"@next/third-parties": "^15.1.3",
		"@prisma/client": "^6.1.0",
		"@types/bcryptjs": "^2.4.6",
		"@types/uuid": "^10.0.0",
		"bcryptjs": "^2.4.3",
		"clsx": "^2.1.1",
		"cron": "^3.5.0",
		"date-fns": "^3.6.0",
		"dotenv-cli": "^7.4.2",
		"next": "^15.1.3",
		"next-auth": "^5.0.0-beta.25",
		"react": "^18",
		"react-cookie": "^7.2.0",
		"react-dom": "^18.3.1",
		"react-hook-form": "^7.51.5",
		"react-icons": "^5.4.0",
		"uuid": "^9.0.1",
		"yup": "^1.4.0"
	},
	"devDependencies": {
		"@types/node": "^20",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"daisyui": "^5.0.0-beta.1",
		"eslint": "^8",
		"eslint-config-next": "14.2.3",
		"postcss": "^8",
		"prettier": "^3.2.5",
		"prisma": "^6.1.0",
		"tailwindcss": "^4.0.0-beta.9",
		"typescript": "^5"
	}
}
